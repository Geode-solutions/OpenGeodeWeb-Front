{
  "$id": "/workflows/crs_selection",
  "type": "object",
  "properties": {
    "search": {
      "type": "string"
    },
    "data_table_loading": {
      "type": "boolean"
    },
    "crs_list": {
      "type": "array",
      "items": {
        "$id": "/workflows/crs",
        "type": "object",
        "properties": {
          "authority": {
            "type": "string"
          },
          "code": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        },
        "required": ["authority", "code", "name"]
      }
    },
    "selected_crs": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": ["search", "data_table_loading", "crs_list", "selected_crs"]
}
